#include <stdio.h>
#include <stdlib.h>


#define COLUMNS 6
int counter = 0;


int is_prime(int n){
  int i;
  for (i = 2; i <= n/2; i++) {
    if (n % i == 0)
      return 0;
  }
  return 1;
}

void print_number(int n){
  printf("%10d\t", n);
  if (counter == COLUMNS){
    counter = 0;
    printf("\n");
  }

}
  
void print_sieves(int n){
  int i;
  int primeList[n];
  for (i = 2; i*i < n ; i++) {
    primeList[i] = 1;
    if (primeList[i]){
      for (j = i << 2; i < n ; i++) {
	
      }

    }
      
  }

  
}



int main(int argc, char *argv[]){
  if(argc == 2)
    print_sieves(atoi(argv[1]));
  else
    printf("Please state an interger number.\n");
  return 0;
}
